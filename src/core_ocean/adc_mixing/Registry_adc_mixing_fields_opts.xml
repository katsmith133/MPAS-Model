    <nml_record name="adcMixingModel" mode="forward">
        <nml_option name="config_use_adc" type="logical" default_value=".false." units="unitless"
            description="If true, the new mass flux RANS closure is called for mixing"
            possible_values=".true. or .false."
            />
        <nml_option name="config_adc_timestep" type="real" default_value="5" units="unitless"
            description="timestep for the turbulence updates and subcycling (now in sec, probably should unify with MPAS later)"
            possible_values="small positive reals (less than MPAS timestep)"
            />
        <nml_option name="config_adc_use_old_length_scale" type="logical" default_value=".false." units="unitless"
            description="flag to use buoyancy length scale from previous closure, temporary"
            possible_values=".true. or .false."
            />
        <nml_option name="config_adc_epsilon" type="real" default_value="1e-8" units="m^2/s^2"
            description="small value below which KE, u2, w2, v2 is not allowed to go below"
            possible_values="very small numbers 1e-8 to zero"
            />
        <nml_option name="config_adc_use_single_column" type="logical" default_value=".false." units="unitless"
            description="if true, ADC code runs on 1 copy and is copied to the others to speed testing"
            possible_values=".true. or .false."
            />
        <nml_option name="config_adc_alpha0" type="real" default_value="0.8" units="unitless"
            description="the first coefficient of the rapid pressure term for momentum"
            possible_values="positive values"
            />
        <nml_option name="config_adc_alpha1" type="real" default_value="0.98" units="unitless"
            description="the second coefficient of the rapid pressure term for momentum"
            possible_values="positive values"
            />
        <nml_option name="config_adc_alpha2" type="real" default_value="0.57" units="unitless"
            description="the third coefficient of the rapid pressure term for momentum"
            possible_values="postive values"
            />
        <nml_option name="config_adc_alpha1_tracer" type="real" default_value="0.6" units="unitless"
            description="the second coefficient of the rapid pressure term for momentum"
            possible_values="positive values"
            />
        <nml_option name="config_adc_alpha2_tracer" type="real" default_value="1.0" units="unitless"
            description="the third coefficient of the rapid pressure term for momentum"
            possible_values="positive values"
            />
        <nml_option name="config_adc_Cb" type="real" default_value="0.4" units="unitless"
            description="coefficient of the buoyancy pressure term for momentum"
            possible_values="positive values"
            />
        <nml_option name="config_adc_Cb_tracer" type="real" default_value="0.333333" units="unitless"
            description="coefficient of the buoyancy pressure term for tracers"
            possible_values="positive values"
            />
        <nml_option name="config_adc_Cb_w3" type="real" default_value="0.1" units="unitless"
            description="coefficient of the buoyancy pressure term for w3"
            possible_values="positive values"
            />
        <nml_option name="config_adc_Cslow" type="real" default_value="2.5" units="unitless"
            description="coefficient of the slow pressure term for momentum"
            possible_values="positive values"
            />
        <nml_option name="config_adc_Cslow_tracer" type="real" default_value="3.0" units="unitless"
            description="coefficient of the slow pressure term for tracers"
            possible_values="positive values"
            />
        <nml_option name="config_adc_Cslow_w3" type="real" default_value="6.5" units="unitless"
            description="coefficient of the slow pressure term for w3"
            possible_values="positive values"
            />
        <nml_option name="config_adc_Cmom" type="real" default_value="0.5" units="unitless"
            description="third order down gradient coefficient, from lappen and randall 2001b"
            possible_values="values near 0.5"
            />
        <nml_option name="config_adc_Ctherm" type="real" default_value="4.85" units="unitless"
            description="third order down gradient for buoyancy moments, from lappen and randall 2001b"
            possible_values="values near 4.85"
            />
        <nml_option name="config_adc_CwwE" type="real" default_value="1.0" units="unitless"
            description="coefficient for entrainment in parameterization from LR01b"
            possible_values="values near 1.0, also CwwD/CwwE must be near 1.5"
            />
        <nml_option name="config_adc_CwwD" type="real" default_value="1.5" units="unitless"
            description="coefficient for detrainment in parameteriztaion from LR01b"
            possible_values="values near 1.0, also CwwD/CwwE must be near 1.5"
            />
        <nml_option name="config_adc_kappaFL" type="real" default_value="0" units="m^2/s"
            description="background diffusion coefficient for fluxes, second moments"
            possible_values="small positive real numbers"
            />
        <nml_option name="config_adc_kappaVAR" type="real" default_value="0" units="m^2/s"
            description="background diffusion coefficient for variances, i.e. T'^2"
            possible_values="small positive real numbers"
            />
        <nml_option name="config_adc_kappaW3" type="real" default_value="0" units="m^2/s"
            description="background diffusion coefficient for w3"
            possible_values="small positive real numbers"
            />
    </nml_record>
